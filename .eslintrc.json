{
	"env": {
		"browser": true,
		"node": true,
		"commonjs": true,
		"es6": true
	},
	"extends": ["airbnb", "prettier"],
	"parser": "babel-eslint",
	"rules": {
		"prettier/prettier": [
			"error",
			{
				"trailingComma": "es5",
				"singleQuote": true,
				"printWidth": 80
			}
		]
	},
	"plugins": ["prettier"]
}
